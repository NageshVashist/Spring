package com.springboot.microservices.currencyconversionservice.bean;

import java.math.BigDecimal;

public class CurrencyConversionBean {
	private Integer id;
	private String from;
	private String to;
	private BigDecimal converisonMultiple;
	private BigDecimal amount;

	public CurrencyConversionBean() {
	}

	public CurrencyConversionBean(Integer id, String from, String to, BigDecimal converisonMultiple,
			BigDecimal amount) {
		super();
		this.id = id;
		this.from = from;
		this.to = to;
		this.converisonMultiple = converisonMultiple;
		this.amount = amount;
	}

}
